<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fecoder.MainViewController">
    <top>
        <HBox prefWidth="200.0" BorderPane.alignment="CENTER" spacing="10">
            <children>
                <Button id="suplierManage" fx:id="suplierManage" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="NHÀ CUNG CẤP" />
                <Button id="packagingTypeManage" fx:id="packagingTypeManage" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="LOẠI BAO BÌ" />
                <Button id="yearManage" fx:id="yearManage" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="NĂM" />
                <Button id="sizeManage" fx:id="sizeManage" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="SIZE TÔM" />
                <Button id="customerManage" fx:id="customerManage" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="KHÁCH HÀNG" />
                <Button id="packagingManage" fx:id="packagingManage" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="BAO BÌ" />
                <Button id="productManage" fx:id="productManage" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="MẶT HÀNG" />
            </children>
            <padding>
                <Insets bottom="10" />
            </padding>
        </HBox>
    </top>
    <center>
      <ScrollPane fitToHeight="true" fitToWidth="true">
         <content>
              <VBox spacing="10.0">
                  <children>
                      <GridPane>
                          <children>
                              <Label text="Session ID:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                              <Label fx:id="sessionLabel" text="Undefined" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                          </children>
                          <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="109.0" />
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="189.0" />
                          </columnConstraints>
                          <rowConstraints>
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                      </GridPane>
                      <StackPane prefHeight="-1.0" prefWidth="-1.0">
                          <children>
                              <HBox alignment="BASELINE_RIGHT" prefHeight="0.0" prefWidth="211.0" spacing="8">
                                  <children>
                                      <Button id="logoutButton" fx:id="logoutButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="Logout" />
                                      <Button id="newWindowButton" fx:id="newWindowButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="New Window" />
                                  </children>
                              </HBox>
                          </children>
                      </StackPane>
                  </children>
              </VBox>
         </content>
      </ScrollPane>
    </center>
    <bottom>
        <HBox prefWidth="200.0" BorderPane.alignment="BOTTOM_CENTER" alignment="BASELINE_RIGHT">
            <children>
                <Button mnemonicParsing="false" text="Button" />
            </children>
            <padding>
                <Insets top="10" />
            </padding>
        </HBox>
    </bottom>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</BorderPane>
