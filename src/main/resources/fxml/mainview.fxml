<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane
        prefHeight="768.0"
        prefWidth="1024.0"
        xmlns="http://javafx.com/javafx/11.0.1"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="fecoder.MainViewController">
    <children>
      <ScrollPane fitToWidth="true" fitToHeight="true">
         <content>
              <VBox spacing="10.0">
                  <children>
                      <GridPane>
                          <children>
                              <Label text="Session ID:" GridPane.columnIndex="0" GridPane.rowIndex="0" />
                              <Label fx:id="sessionLabel" text="Undefined" GridPane.columnIndex="1" GridPane.rowIndex="0" />
                          </children>
                          <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="148.0" minWidth="10.0" prefWidth="109.0" />
                              <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="10.0" prefWidth="189.0" />
                          </columnConstraints>
                          <rowConstraints>
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                      </GridPane>
                      <StackPane prefHeight="-1.0" prefWidth="-1.0">
                          <children>
                              <HBox alignment="BASELINE_RIGHT" prefHeight="0.0" prefWidth="211.0" spacing="8">
                                  <children>
                                      <Button id="logoutButton" fx:id="logoutButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="Logout" />
                                      <Button id="newWindowButton" fx:id="newWindowButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="New Window" />
                                  </children>
                              </HBox>
                          </children>
                      </StackPane>
                  </children>
              </VBox>
         </content>
      </ScrollPane>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</StackPane>
